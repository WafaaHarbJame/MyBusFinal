<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingEnd="@dimen/_8sdp">

    <LinearLayout
        android:id="@+id/rowLY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle_corner_gray_fill"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingStart="@dimen/_10sdp"
        android:paddingTop="@dimen/_6sdp"
        android:paddingEnd="@dimen/_10sdp"
        android:paddingBottom="@dimen/_6sdp">

        <com.elaj.patient.spichalViews.STextViewIcon
            android:layout_width="@dimen/_13sdp"
            android:layout_height="@dimen/_13sdp"
            android:layout_marginEnd="@dimen/_10sdp"
            android:background="@drawable/circle_primary_fill"
            android:gravity="center"
            android:text="@string/fal_check"
            android:textColor="@color/white"
            android:textSize="@dimen/_9ssp" />

        <com.elaj.patient.spichalViews.STextView
            android:id="@+id/nameTxt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Has Stress"
            android:textAlignment="viewStart"
            android:textColor="@color/colorAccent"
            android:textSize="@dimen/_12ssp" />

    </LinearLayout>

</LinearLayout>